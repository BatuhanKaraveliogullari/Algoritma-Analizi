
package power;

public class Power {

    public static int carpim = 1;
    
    
    public static int ussAl(int a,int b)
    {
         int sayac = 0;
        for (int i = 0; i < b; i++) {
            carpim = carpim * a;           
            sayac++;
            
        }
        System.out.println(sayac);
        return carpim;
        
    }
    
    
    
    public static void main(String[] args) {
        System.out.println(ussAl(3,62));
    }
    
}
